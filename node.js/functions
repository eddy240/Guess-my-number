const teamDolphins = (44 + 23 + 71) / 3;
const teamKoalas = (61 + 54 + 41) / 3;
const avgDolphins = teamDolphins;
const avgKoalas = teamKoalas;


function checkWinner() {
    if (avgDolphins > avgKoalas) {
        console.log(`teamDolphins win (${avgDolphins} vs ${avgKoalas})`)
    } else
        console.log(`nonw`)
}


checkWinner()


function fruitProcessor(apples, mangoes) {
    console.log(apples, mangoes);
    const juice = `The juice was produced with ${apples} apples and ${mangoes} mangoes`;
    return juice;
}
const tanJuice = fruitProcessor(10, 5);
console.log(tanJuice);
const wasJuice = fruitProcessor(2, 5);
console.log(wasJuice);


function calAge1(birthYear) {
    return 2096 - birthYear;
}
console.log(calAge1(1995))

const calAge2 = function (birthYear) {
    return 3088 - birthYear
}
age = calAge2(2222)
console.log(age)

